  turtle.end_fill()
def cherry_bush():
  turtle.color((330,112,147))
  turtle.begin_fill()
  turtle.forward(30)
  turtle.right(90)
  turtle.forward(30)
  turtle.right(90)
  turtle.forward(30)
  turtle.right(90)
  turtle.forward(30)
  turtle.right(90)
  turtle.end_fill()
def cherry_grass_one():
  cherry_grass_one_size = randint(100,150)
  turtle.color((219,112,147))
  turtle.setheading(0)
  turtle.begin_fill()
  turtle.forward(cherry_grass_one_size)
  turtle.left(90)
  turtle.forward(cherry_grass_one_size)
  turtle.left(90)
  turtle.forward(cherry_grass_one_size)
  turtle.left(90)
  turtle.forward(cherry_grass_one_size)
  turtle.left(90)
  turtle.end_fill()
def cherry_grass_two():
  cherry_grass_two_size = randint(100,150)
  turtle.color((219,112,110))
  turtle.setheading(0)
  turtle.begin_fill()
  turtle.forward(cherry_grass_two_size)
  turtle.left(90)
  turtle.forward(cherry_grass_two_size)
  turtle.left(90)
  turtle.forward(cherry_grass_two_size)
  turtle.left(90)
  turtle.forward(cherry_grass_two_size)
  turtle.left(90)
  turtle.end_fill()
def cherrytree_forest():
  MushX = randint(-1000,1000)
  MushY = randint(-1000,1000)
  MushSize = randint(500,1000)
  MushXline = MushX + MushSize
  MushYline = MushY + MushSize
  turtle.up()
  turtle.goto(MushX,MushY)
  turtle.down()
  turtle.color((60,140,70))
  turtle.up()
  turtle.begin_fill()
  turtle.setheading(0)
  turtle.down()
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.right(90)
  turtle.end_fill()
  for i in range(randint(10,20)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    cherry_grass_one()
  for i in range(randint(10,20)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    cherry_grass_two()
  for i in range(randint(30,35)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    cherry_bush()
  for i in range(randint(5,20)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    cherry_tree()
def Mushroom_forest():
  MushX = randint(-1000,1000)
  MushY = randint(-1000,1000)
  MushSize = randint(500,1000)
  MushXline = MushX + MushSize
  MushYline = MushY + MushSize
  turtle.up()
  turtle.goto(MushX,MushY)
  turtle.down()
  turtle.color("grey")
  turtle.up()
  turtle.begin_fill()
  turtle.setheading(0)
  turtle.down()
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.right(90)
  turtle.goto(MushX,MushY)
  turtle.end_fill()
  
  for i in range(randint(5,27)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    Mushroom_red()
  for i in range(randint(4,24)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    brown_mushroom()
