    turtle.down()
    redwood_tree()
def ocean():
  MushX = randint(-1000,1000)
  MushY = randint(-1000,1000)
  MushSize = randint(500,1000)
  MushXline = MushX + MushSize
  MushYline = MushY + MushSize
  OceanLeft = 90
  turtle.up()
  turtle.goto(MushX,MushY)
  turtle.down()
  turtle.color((50,100,200))
  turtle.up()
  turtle.begin_fill()
  turtle.setheading(0)
  turtle.down()
  turtle.forward(MushSize)
  turtle.left(OceanLeft)
  turtle.forward(MushSize)
  turtle.left(OceanLeft)
  turtle.forward(MushSize)
  turtle.left(OceanLeft)
  turtle.forward(MushSize)
  turtle.right(OceanLeft)
  turtle.end_fill()
def Forest():
  MushX = randint(-1000,1000)
  MushY = randint(-1000,1000)
  MushSize = randint(500,1000)
  MushXline = MushX + MushSize
  MushYline = MushY + MushSize
  turtle.up()
  turtle.goto(MushX,MushY)
  turtle.down()
  turtle.color((60,140,70))
  turtle.up()
  turtle.begin_fill()
  turtle.setheading(0)
  turtle.down()
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.left(90)
  turtle.forward(MushSize)
  turtle.right(90)
  turtle.end_fill()
  for i in range(randint(10,20)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    podtzal()
  for i in range(randint(10,20)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    beech_tree()
  for i in range(randint(21,30)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    oak_tree()
  for i in range(randint(2,3)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    redwood_tree()
  for i in range(randint(4,8)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    Mushroom_red()
  for i in range(randint(4,8)):
    RedMushX = randint(MushX,MushXline)
    RedMushY = randint(MushY,MushYline)
    turtle.up()
    turtle.goto(RedMushX,RedMushY)
    turtle.down()
    brown_mushroom()
turtle.goto(-10000,-10000)
turtle.color((50,100,200))
turtle.begin_fill()
for i in range(4):
  turtle.forward(20000)
  turtle.left(90)
turtle.end_fill()
Forest()
redwood_forest()
Mushroom_forest()
cherrytree_forest()
