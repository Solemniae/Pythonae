import turtle
turtle.tracer(120)
turtle.speed("0")
from random import randint
def beech_tree():
  turtle.setheading(0)
  turtle.color("white")
  turtle.begin_fill()
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.end_fill()
  turtle.left(90)
  turtle.forward(30)
  turtle.color("green")
  turtle.begin_fill()
  turtle.left(90)
  turtle.forward(10)
  turtle.right(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(10)
  turtle.right(90)
  turtle.forward(10)
  turtle.right(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(10)
  turtle.right(90)
  turtle.forward(30)
  turtle.right(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(10)
  turtle.right(90)
  turtle.forward(10)
  turtle.right(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(10)
  turtle.right(90)
  turtle.forward(20)
  turtle.end_fill()
def oak_tree():
  turtle.setheading(0)
  turtle.color((160,82,45))
  turtle.begin_fill()
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
