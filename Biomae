  turtle.begin_fill()
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(10)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(10)
  turtle.end_fill()
def boulder():
  boulder_size = randint(50,100)
  turtle.color("grey")
  turtle.setheading(0)
  turtle.begin_fill()
  turtle.forward(boulder_size)
  turtle.left(90)
  turtle.forward(boulder_size)
  turtle.left(90)
  turtle.forward(boulder_size)
  turtle.left(90)
  turtle.forward(boulder_size)
  turtle.left(90)
  turtle.end_fill()
def redwood_bush():
  turtle.color("dark green")
  turtle.begin_fill()
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.end_fill()
def berry_bush():
  turtle.setheading(0)
  turtle.color("darkgreen")
  turtle.begin_fill()
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.forward(30)
  turtle.left(90)
  turtle.end_fill()
  turtle.up()
  turtle.left(45)
  turtle.forward(10)
  turtle.down()
  turtle.color("red")
  turtle.begin_fill()
  turtle.forward(5)
  turtle.left(90)
  turtle.forward(5)
  turtle.left(90)
  turtle.forward(5)
  turtle.left(90)
  turtle.forward(5)
  turtle.left(90)
  turtle.end_fill()
  turtle.up()
  turtle.forward(15)
  turtle.down()
  turtle.color("red")
  turtle.begin_fill()
  turtle.forward(5)
  turtle.left(90)
  turtle.forward(5)
  turtle.left(90)
  turtle.forward(5)
  turtle.left(90)
  turtle.forward(5)
  turtle.left(90)
  turtle.end_fill()
  turtle.up()
